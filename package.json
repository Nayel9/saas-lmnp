{
  "name": "lmnp-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
  "dev": "concurrently \"next dev --turbopack\" \"pnpm:css:dev\"",
  "build": "pnpm css:build && next build",
  "start": "next start",
  "css:dev": "pnpm dlx tailwindcss -i ./src/app/globals.css -o ./public/tailwind.css --watch",
  "css:build": "pnpm dlx tailwindcss -i ./src/app/globals.css -o ./public/tailwind.css --minify",
  "db:generate": "prisma generate",
  "db:migrate": "prisma migrate dev",
  "db:studio": "prisma studio",
  "lint": "next lint"
  },
  "dependencies": {
    "@mantine/core": "^8.2.4",
    "@mantine/hooks": "^8.2.4",
    "@mantine/notifications": "^8.2.4",
    "@prisma/client": "^6.14.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
